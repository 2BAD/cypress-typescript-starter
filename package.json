{
  "name": "cypress-starter",
  "version": "1.0.0",
  "description": "> Example Cypress with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/2BAD/cypress-starter.git"
  },
  "license": "MIT",
  "author": "2BAD (https://github.com/2BAD)",
  "contributors": [
    {
      "name": "Jason Hyde",
      "email": "github@2bad.me",
      "url": "https://github.com/mrhyde"
    }
  ],
  "scripts": {
    "cy:docker": "docker run -it -w /e2e -v $PWD:/e2e",
    "cy:docker:open": "cy:docker -v /tmp/.X11-unix:/tmp/.X11-unix -e DISPLAY --entrypoint cypress cypress/included:11.2.0 open --project .",
    "cy:docker:run": "run-s cy:docker cypress/included:11.2.0 --browser chrome",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:headed": "cypress run --headed",
    "cy:run:record": "cypress run --record",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "lint": "eslint . --ext .ts"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@cypress/grep": "3.1.5",
    "@tsconfig/cypress": "1.0.1",
    "@types/node": "18.15.10",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "cypress": "12.8.1",
    "cypress-each": "1.13.1",
    "cypress-multi-reporters": "1.6.3",
    "cypress-repeat": "2.3.3",
    "cypress-visual-regression": "3.0.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.7",
    "typescript": "5.0.2"
  },
  "volta": {
    "node": "19.8.1"
  }
}
